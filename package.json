{
  "name": "open-tabs",
  "displayName": "Open tabs",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "prettier": "3.2.4",
    "tailwindcss": "3.4.3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "options_page": "options.html",
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "permissions": [
      "storage",
      "tabs",
      "tabGroups",
      "bookmarks",
      "contextMenus",
      "notifications"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ]
  }
}
